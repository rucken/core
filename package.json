{
  "name": "rucken",
  "version": "1.27.1",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "description": "Core with Admin UI for web and native application maked on Angular5+",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular5",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "lib-core:clear": "./node_modules/.bin/del-cli ./libs/core/src/node_modules ./libs/core/dist ./libs/core/.tmp",
    "lib-core:link": "ngm link -p ./libs/core/src --here",
    "lib-core:build": "ngm build -p ./libs/core/src --clean",
    "lib-core:link-npm": "npm link ./libs/core/src",
    "lib-core:link-npm-clear": "./node_modules/.bin/del-cli ./libs/core/src/node_modules ./libs/core/dist/node_modules",
    "lib-core:tools-change_version": "gulp lib-core:change-version",
    "lib-core:tools-extract_translate": "ngx-translate-extract --input ./libs/core/src --output ./libs/core/src/i18n/template.pot --format=pot --marker translate --clean",
    "lib-core:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/core-convert.po.to.ts.json",
    "lib-core:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/core-make.list.ts.files.json",
    "lib-core:tools-prepare": "npm-run-all lib-core:tools-extract_translate lib-core:tools-po2ts lib-core:tools-make_ts_list",
    "lib-core:pack": "npm pack ./libs/core/dist",
    "lib-web:clear": "./node_modules/.bin/del-cli ./libs/web/src/node_modules ./libs/web/dist ./libs/web/.tmp",
    "lib-web:link": "ngm link -p ./libs/web/src --here",
    "lib-web:build": "ngm build -p ./libs/web/src --clean",
    "lib-web:link-npm": "npm link ./libs/web/src",
    "lib-web:link-npm-clear": "./node_modules/.bin/del-cli ./libs/web/src/node_modules ./libs/web/dist/node_modules",
    "lib-web:tools-change_version": "gulp lib-web:change-version",
    "lib-web:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/web-convert.po.to.ts.json",
    "lib-web:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/web-make.list.ts.files.json",
    "lib-web:tools-extract_translate": "ngx-translate-extract --input ./libs/web/src --output ./libs/web/src/i18n/template.pot --format=pot --marker translate --clean",
    "lib-web:tools-prepare": "npm-run-all lib-web:tools-extract_translate lib-web:tools-po2ts lib-web:tools-make_ts_list",
    "lib-web:pack": "npm pack ./libs/web/dist",
    "libs:clear": "npm-run-all lib-core:clear lib-web:clear",
    "libs:change_version": "npm-run-all lib-core:tools-change_version lib-web:tools-change_version",
    "libs:prepare": "npm-run-all lib-core:tools-prepare lib-web:tools-prepare",
    "libs:pack": "npm-run-all lib-core:pack lib-web:pack",
    "libs:build": "npm-run-all libs:clear libs:change_version lib-core:build lib-core:link lib-web:build lib-core:pack lib-web:pack",
    "libs:link": "npm-run-all lib-core:link lib-web:link lib-core:link-npm-clear lib-web:link-npm-clear",
    "libs:link-npm": "npm-run-all lib-core:link-npm lib-web:link-npm lib-core:link-npm-clear lib-web:link-npm-clear",
    "app-demo:clear": "./node_modules/.bin/del-cli ./apps/demo/src/node_modules ./apps/demo/dist ./apps/demo/.tmp",
    "app-demo:github_pages": "npm-run-all app-demo:github_full_build app-demo:github_deploy",
    "app-demo:github_build": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --base-href https://rucken.github.io/core/ --prod --aot --env=mockapi --build-optimizer",
    "app-demo:github_full_build": "npm-run-all libs:build libs:link app-demo:github_build app-demo:tools-add_version",
    "app-demo:github_deploy": "gh-pages -d apps/demo/dist",
    "app-demo:lite_server": "lite-server -c bs-config.json",
    "app-demo:serve_dev": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot=false",
    "app-demo:serve_prod": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot",
    "app-demo:serve_mockapi": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot --env=mockapi",
    "app-demo:prod_build": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --base-href / --prod --aot --build-optimizer",
    "app-demo:prod_full_build": "npm-run-all libs:build libs:link app-demo:prod_build app-demo:tools-add_version",
    "app-demo:tools-add_version": "gulp app-demo:add-version",
    "app-demo:start-dev": "npm-run-all libs:prepare libs:link-npm app-demo:serve_dev",
    "app-demo:start-prod": "npm-run-all libs:prepare libs:link-npm app-demo:serve_prod",
    "app-demo:start-mockapi": "npm-run-all libs:prepare libs:link-npm app-demo:serve_mockapi",
    "app-demo:start-full_dev": "npm-run-all libs:prepare libs:build libs:link app-demo:serve_dev",
    "app-demo:start-full_prod": "npm-run-all libs:prepare libs:build libs:link app-demo:serve_prod",
    "app-demo:start-full_mockapi": "npm-run-all libs:prepare libs:build libs:link app-demo:serve_mockapi",
    "app-demo:tools-extract_translate": "ngx-translate-extract --input ./apps/demo/src/app --output ./apps/demo/src/app/i18n/template.pot --format=pot --marker translate --clean",
    "app-demo:tools-po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/demo-convert.po.to.ts.json",
    "app-demo:tools-make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/demo-make.list.ts.files.json",
    "app-demo:tools-prepare": "npm-run-all app-demo:tools-extract_translate app-demo:tools-po2ts app-demo:tools-make_ts_list",
    "app-demo:e2e": "ng e2e --app=demo --preserve-symlinks",
    "apps:clear": "npm-run-all app-demo:clear",
    "apps:prepare": "npm-run-all app-demo:tools-prepare",
    "all:clear": "npm-run-all libs:clear apps:clear",
    "all:prepare": "npm-run-all libs:prepare apps:prepare",
    "gh:build": "npm-run-all all:clear all:prepare format app-demo:github_full_build",
    "gh:deploy": "npm-run-all app-demo:github_deploy",
    "ng": "ng",
    "lint": "ng lint",
    "format": "tsfmt -r",
    "test": "npm-run-all lint app-demo:prod_full_build",
    "build": "npm-run-all app-demo:prod_full_build",
    "deploy": "npm-run-all app-demo:github_deploy",
    "start": "npm-run-all app-demo:start-mockapi"
  },
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-webworker": "^5.0.2",
    "@angular/platform-webworker-dynamic": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@ngui/auto-complete": "^0.16.0",
    "@ngx-translate/core": "^9.0.0",
    "@types/jasmine": "2.8.2",
    "@types/lodash": "^4.14.85",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.53",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.8.1",
    "angular2-ladda": "^1.2.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.4.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "ngx-bootstrap": "^2.0.0-beta.8",
    "rxjs": "^5.5.2",
    "text-mask-addons": "^3.7.1",
    "tslib": "^1.8.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.2",
    "@angular/compiler-cli": "^5.0.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.0.1",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.3",
    "del-cli": "^1.1.0",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "ngm-cli": "^0.8.3",
    "node-sass": "^4.7.1",
    "npm-run-all": "^4.1.2",
    "po2json": "^0.4.5",
    "protractor": "~5.2.0",
    "recursive-readdir": "^2.2.1",
    "shebang-command": "^1.2.0",
    "srcgen": "^0.3.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "^2.4.2",
    "typescript-formatter": "^7.0.0",
    "typings": "^2.1.1"
  }
}
