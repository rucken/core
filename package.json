{
  "name": "@rucken/core",
  "version": "1.13.2",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "description": "Core with Admin UI for web and native application maked on Angular4",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular4",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "build-core": "ngm build -p ./core --clean",
    "link-core": "ngm link -p core --here",
    "demo-link-core": "npm link core",
    "tools-change_core_version": "gulp change-core-version",
    "tools-extract_translate-src-core": "ngx-translate-extract --input ./core --output ./core/i18n/template.pot --format=pot --marker translate --clean",
    "tools-po2ts-core": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/core-convert.po.to.ts.json",
    "tools-make_ts_list-core": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/core-make.list.ts.files.json",
    "tools-prepare-core": "npm-run-all tools-extract_translate-src-core tools-po2ts-core tools-make_ts_list-core",

    "link-src": "ngm link -p src --here",
    "build-src": "ngm build -p ./src --clean",
    "demo-link-src": "npm link src",
    "tools-change_src_version": "gulp change-src-version",
    "tools-po2ts-src": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/convert.po.to.ts.json",
    "tools-make_ts_list-src": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/make.list.ts.files.json",
    "tools-extract_translate-src": "ngx-translate-extract --input ./src --output ./src/i18n/template.pot --format=pot --marker translate --clean",
    "tools-prepare-src": "npm-run-all tools-extract_translate-src tools-po2ts-src tools-make_ts_list-src",

    "build": "npm-run-all tools-change_version build-core link-core build-src",
    "ng": "ng",
    "test": "npm-run-all lint demo-build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "link": "npm-run-all link-core link-src",
    "start": "npm-run-all tools-prepare demo-link demo-serve",
    "mockapi": "npm-run-all tools-prepare demo-link demo-serve_mockapi",
    "demo-link": "npm-run-all demo-link-core demo-link-src",
    "demo-lite_server": "lite-server -c demo/bs-config.json",
    "demo-serve": "ng serve --host 0.0.0.0 --aot",
    "demo-serve_mockapi": "ng serve --host 0.0.0.0 --aot --env=mockapi",
    "demo-gh_pages": "npm-run-all demo-build demo-deploy",
    "demo-prod_build": "ng build --base-href https://rucken.github.io/core/ --prod --aot --build-optimizer",
    "demo-dev_build": "ng build --base-href / --aot",
    "demo-deploy": "gh-pages -d demo/dist",
    "demo-build": "npm-run-all build link demo-prod_build tools-demo-add_version",
    "tools-demo-add_version": "gulp add-version",
    "tools-change_version": "npm-run-all tools-change_core_version tools-change_src_version",
    "tools-prepare": "npm-run-all tools-prepare-core tools-prepare-src"
  },
  "dependencies": {
    "@angular/animations": "^4.4.3",
    "@angular/common": "^4.4.3",
    "@angular/compiler": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/forms": "^4.4.3",
    "@angular/http": "^4.4.3",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "@angular/router": "^4.4.3",
    "@ngui/auto-complete": "^0.15.0",
    "@ngx-translate/core": "^8.0.0",
    "@types/jasmine": "2.6.0",
    "@types/lodash": "^4.14.76",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.31",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.0.12",
    "angular2-jwt": "^0.2.3",
    "angular2-ladda": "^1.2.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.2.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ngx-bootstrap": "^1.9.3",
    "rxjs": "^5.4.3",
    "text-mask-addons": "^3.6.0",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~3.2.0",
    "conventional-changelog-cli": "^1.3.3",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.1",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "ngm-cli": "^0.7.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "po2json": "^0.4.5",
    "protractor": "~5.1.2",
    "recursive-readdir": "^2.2.1",
    "shebang-command": "^1.2.0",
    "srcgen": "^0.3.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.3",
    "typings": "^2.1.1"
  }
}
