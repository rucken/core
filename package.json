{
  "name": "rucken",
  "version": "1.30.2",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "description": "Core with Admin UI for web and native application maked on Angular5+",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular5",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "libs:build": "node ./node_modules/@rucken/cli/bin/rucken commands build ~~lib @rucken/core link ~~lib @rucken/core build ~~lib @rucken/web --verbose",
    "libs:link": "node ./node_modules/@rucken/cli/bin/rucken commands link ~~lib",
    "libs:link-npm": "node ./node_modules/@rucken/cli/bin/rucken commands link-npm ~~lib",
    "app:serve-dev": "node ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --aot",
    "app:serve-prod": "node ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --aot --env=prod",
    "app:serve-mockapi": "node ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --aot --env=mockapi",
    "app:start-dev": "npm-run-all clear prepare format libs:link-npm app:serve-dev",
    "app:start-prod": "npm-run-all clear prepare format libs:link-npm app:serve-prod",
    "app:start-mockapi": "npm-run-all clear prepare format libs:link-npm app:serve-mockapi",
    "app:start-full_dev": "npm-run-all clear prepare format libs:build libs:link app:serve-dev",
    "app:start-full_prod": "npm-run-all clear prepare format libs:build libs:link app:serve-prod",
    "app:start-full_mockapi": "npm-run-all clear prepare format libs:build libs:link app:serve-mockapi",
    "app:build-prod": "node ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --prod --build-optimizer",
    "app:build-full_prod": "npm-run-all clear prepare format libs:build libs:link app:build-prod",
    "app:e2e": "ng e2e --app=demo --preserve-symlinks",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "lint": "ng lint",
    "format": "tsfmt -r",
    "clear": "node ./node_modules/@rucken/cli/bin/rucken clear",
    "prepare": "node ./node_modules/@rucken/cli/bin/rucken prepare",
    "test": "npm-run-all lint app:build-full_prod",
    "e2e": "ng e2e --app=demo --preserve-symlinks",
    "build": "npm-run-all app:build-full_prod",
    "start": "npm-run-all app:start-mockapi",
    "gh:build-mockapi": "node ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --base-href https://rucken.github.io/core/ --prod --env=mockapi --build-optimizer",
    "gh:build": "npm-run-all clear prepare format libs:build libs:link gh:build-mockapi",
    "gh:deploy": "gh-pages -d apps/demo/dist"
  },
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/platform-webworker": "5.2.0",
    "@angular/platform-webworker-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "@types/jasmine": "2.8.4",
    "@types/lodash": "^4.14.92",
    "@types/marked": "0.3.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.3.0",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.8.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "ngx-bootstrap": "^2.0.0-rc.0",
    "rxjs": "^5.5.6",
    "text-mask-addons": "^3.7.1",
    "tslib": "^1.8.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "5.2.0",
    "@rucken/cli": "^1.7.5",
    "codelyzer": "~4.0.2",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.1.0",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.1.0",
    "html-loader": "0.5.4",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.2.2",
    "shebang-command": "^1.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.5.3",
    "typescript-formatter": "^7.0.1",
    "typings": "^2.1.1"
  }
}
