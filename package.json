{
  "name": "rucken",
  "version": "1.10.0",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "bugs": {
    "url": "https://github.com/site15/rucken/issues"
  },
  "private": true,
  "description": "Core with Admin UI for web application maked on Angular4",
  "homepage": "https://github.com/site15/rucken",
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular4",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/site15/rucken.git"
  },
  "scripts": {
    "build.change-src-version": "gulp change-src-version",
    "build.inline-templates": "gulp inline-templates",
    "build.build-src-inline": "ngm build -p ./src-inline --clean",
    "build": "npm-run-all build.change-src-version build.inline-templates build.build-src-inline",
    "ng": "ng",
    "start": "ng serve --aot=false --host 0.0.0.0",
    "test": "npm-run-all lint build demo.build demo.add-version",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lite-server": "lite-server -c demo/bs-config.json",
    "link": "ngm link -p src --here",
    "demo.serve": "npm-run-all build link demo.build lite-server",
    "demo.gh-pages": "npm-run-all build demo.build demo.add-version demo.deploy",
    "demo.build": "ng build --base-href https://site15.github.io/rucken/ --prod --aot=false",
    "demo.add-version": "gulp add-version",
    "demo.deploy": "gh-pages -d demo/dist",
    "_extract_translate": "ngx-translate-extract --input ./src --output ./src/i18n/template.pot --format=pot --marker translate --clean",
    "_po2ts": "./node_modules/.bin/srcgen -x -t convert.po.to.ts -f ./srcgen/convert.po.to.ts.json",
    "_make_ts_list": "./node_modules/.bin/srcgen -x -t make.list.ts.files -f ./srcgen/make.list.ts.files.json",
    "_prepare": "npm-run-all _extract_translate _po2ts _make_ts_list"
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@ngui/auto-complete": "^0.13.3",
    "@ngx-translate/core": "^7.0.0",
    "@types/jasmine": "2.5.52",
    "@types/lodash": "^4.14.66",
    "@types/marked": "0.0.28",
    "@types/node": "^8.0.1",
    "@types/requirejs": "^2.1.29",
    "@types/webpack": "^2.2.16",
    "angular2-jwt": "^0.2.3",
    "angular2-ladda": "^1.2.1",
    "angular2-text-mask": "^8.0.2",
    "awesome-typescript-loader": "^3.1.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^1.7.1",
    "rxjs": "^5.4.1",
    "text-mask-addons": "^3.6.0",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.2.4",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~3.1.1",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.0",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-replace": "^0.6.1",
    "gulp-tsc": "^1.3.2",
    "html-loader": "0.4.5",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "markdown-loader": "^2.0.0",
    "ngm-cli": "^0.5.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "po2json": "^0.4.5",
    "protractor": "~5.1.2",
    "recursive-readdir": "^2.2.1",
    "srcgen": "^0.2.13",
    "ts-node": "~3.1.0",
    "tslint": "~5.4.3",
    "typescript": "~2.3.4",
    "typings": "^2.1.1"
  }
}
