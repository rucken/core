{
  "name": "rucken",
  "version": "1.28.1",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3.0"
  },
  "private": true,
  "description": "Core with Admin UI for web and native application maked on Angular5+",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "main": "bundles/rucken.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ngx",
    "angular5",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "libs:build": "node ./node_modules/@rucken/cli/bin/rucken commands build ~~lib @rucken/core link ~~lib @rucken/core build ~~lib",
    "libs:link": "node ./node_modules/@rucken/cli/bin/rucken commands link ~~lib",
    "libs:link-npm": "node ./node_modules/@rucken/cli/bin/rucken commands link-npm ~~lib",
    "all:prepare": "node ./node_modules/@rucken/cli/bin/rucken commands clear prepare",
    "app-demo:github_pages": "npm-run-all app-demo:github_full_build app-demo:github_deploy",
    "app-demo:github_build": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --base-href https://rucken.github.io/core/ --prod --aot --env=mockapi --build-optimizer",
    "app-demo:github_full_build": "npm-run-all app-demo:prepare libs:build libs:link app-demo:github_build app-demo:prepare",
    "app-demo:github_deploy": "gh-pages -d apps/demo/dist",
    "app-demo:lite_server": "lite-server -c bs-config.json",
    "app-demo:serve_dev": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot=false",
    "app-demo:serve_prod": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot",
    "app-demo:serve_mockapi": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --app=demo --preserve-symlinks --host 0.0.0.0 --aot --env=mockapi",
    "app-demo:prod_build": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --app=demo --preserve-symlinks --base-href / --prod --aot --build-optimizer",
    "app-demo:prod_full_build": "npm-run-all app-demo:prepare libs:build libs:link app-demo:prod_build app-demo:prepare",
    "app-demo:start-dev": "npm-run-all all:prepare libs:link-npm app-demo:serve_dev",
    "app-demo:start-prod": "npm-run-all all:prepare libs:link-npm app-demo:serve_prod",
    "app-demo:start-mockapi": "npm-run-all all:prepare libs:link-npm app-demo:serve_mockapi",
    "app-demo:start-full_dev": "npm-run-all all:prepare libs:build libs:link app-demo:serve_dev",
    "app-demo:start-full_prod": "npm-run-all all:prepare libs:build libs:link app-demo:serve_prod",
    "app-demo:start-full_mockapi": "npm-run-all all:prepare libs:build libs:link app-demo:serve_mockapi",
    "app-demo:prepare": "node ./node_modules/@rucken/cli/bin/rucken prepare --app demo",
    "app-demo:e2e": "ng e2e --app=demo --preserve-symlinks",
    "gh:build": "npm-run-all all:prepare format app-demo:github_full_build",
    "gh:deploy": "npm-run-all app-demo:github_deploy",
    "ng": "ng",
    "lint": "ng lint",
    "format": "tsfmt -r",
    "test": "npm-run-all lint app-demo:prod_full_build",
    "build": "npm-run-all app-demo:prod_full_build",
    "deploy": "npm-run-all app-demo:github_deploy",
    "start": "npm-run-all app-demo:start-mockapi"
  },
  "dependencies": {
    "@angular/animations": "5.0.5",
    "@angular/common": "5.0.5",
    "@angular/compiler": "5.0.5",
    "@angular/core": "5.0.5",
    "@angular/forms": "5.0.5",
    "@angular/http": "5.0.5",
    "@angular/platform-browser": "5.1.0",
    "@angular/platform-browser-dynamic": "5.0.5",
    "@angular/platform-webworker": "5.0.5",
    "@angular/platform-webworker-dynamic": "5.0.5",
    "@angular/router": "5.0.5",
    "@ngui/auto-complete": "^0.16.0",
    "@ngx-translate/core": "^9.0.1",
    "@types/jasmine": "2.8.2",
    "@types/lodash": "^4.14.87",
    "@types/marked": "0.3.0",
    "@types/node": "^8.0.55",
    "@types/requirejs": "^2.1.31",
    "@types/webpack": "^3.8.1",
    "angular2-ladda": "^1.2.2",
    "angular2-text-mask": "^8.0.4",
    "awesome-typescript-loader": "^3.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "ngx-bootstrap": "^2.0.0-beta.10",
    "rxjs": "^5.5.5",
    "text-mask-addons": "^3.7.1",
    "tslib": "^1.8.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.5",
    "@angular/compiler-cli": "5.0.5",
    "@rucken/cli": "^1.4.0",
    "codelyzer": "~4.0.1",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^1.0.3",
    "es6-promise-loader": "^1.0.2",
    "gh-pages": "^1.1.0",
    "html-loader": "0.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "locate-path": "^2.0.0",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.2.0",
    "shebang-command": "^1.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "2.4.2",
    "typescript-formatter": "^7.0.0",
    "typings": "^2.1.1"
  }
}
