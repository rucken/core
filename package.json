{
  "name": "rucken",
  "version": "3.0.2",
  "engines": {
    "node": "8",
    "npm": ">=6.1.0"
  },
  "private": true,
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "description": "Core with Admin UI for web and native application maked on Angular6+",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "keywords": [
    "ngx",
    "angular6",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap",
    "ssr"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:dev": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:dev": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-core": "ng build @rucken/core --prod",
    "lib:build-web": "ng build @rucken/web --prod",
    "lib:build": "run-s prepare:prod lib:build-core lib:build-web",
    "app:prebuild": "ng build demo --prod",
    "app:prebuild-dev": "ng build demo --prod -c=dev",
    "app:build": "run-s lib:build app:prebuild",
    "app:build-dev": "run-s lib:build app:prebuild-dev",
    "app:build:mono": "run-s prepare:dev app:prebuild",
    "app:build-dev:mono": "run-s prepare:dev app:prebuild-dev",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "serve-dev": "ng serve -c=dev",
    "start": "run-s ssr:start",
    "run": "run-s prepare:dev serve",
    "run-dev": "run-s prepare:dev serve-dev",
    "build": "run-s config:dev app:build",
    "build-dev": "run-s config:prod app:build-dev",
    "test": "npm run app:build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ssr:prebuild": "ng run demo:server:production",
    "ssr:prebuild-dev": "ng run demo:server:dev",
    "ssr:webpack": "webpack --progress --colors",
    "ssr:start": "node ./server.js",
    "ssr:build": "run-s app:build ssr:prebuild ssr:webpack",
    "ssr:build-dev": "run-s app:build-dev ssr:prebuild-dev ssr:dev",
    "ssr:build:mono": "run-s app:build:mono ssr:prebuild ssr:webpack",
    "ssr:build-dev:mono": "run-s app:build-dev:mono ssr:prebuild-dev ssr:webpack",
    "heroku-postbuild": "run-s ssr:build-mockapi",
    "postinstall": "node ./scripts/patch.js",
    "app:prebuild-mockapi": "ng build demo --prod -c=mockapi",
    "app:prebuild-github": "ng build demo --base-href /core/ --prod -c=mockapi",
    "app:build-mockapi": "run-s lib:build app:prebuild-mockapi",
    "app:build-github": "run-s lib:build app:prebuild-github",
    "app:build-mockapi:mono": "run-s prepare:prod app:prebuild-mockapi",
    "serve-mockapi": "ng serve -c=mockapi",
    "run-mockapi": "run-s prepare:dev serve-mockapi",
    "build-mockapi": "run-s config:prod app:build-mockapi",
    "prebuild-github": "cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "build-github": "run-s config:prod app:build-github prebuild-github",
    "deploy-github": "gh-pages -d dist/demo",
    "ssr:prebuild-mockapi": "ng run demo:server:mockapi",
    "ssr:build-mockapi": "run-s app:build-mockapi ssr:prebuild-mockapi ssr:webpack",
    "ssr:build-mockapi:mono": "run-s app:build-mockapi:mono ssr:prebuild-mockapi ssr:webpack"
  },
  "postinstall": "node scripts/patch.js",
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/platform-server": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "core-js": "^2.5.7",
    "cors": "~2.8.4",
    "@types/node": "~8.9.4",
    "dotenv": "^6.0.0",
    "@types/dotenv": "^4.0.3",
    "@ngx-translate/core": "^10.0.2",
    "jsonwebtoken": "^8.3.0",
    "@types/jsonwebtoken": "^7.2.8",
    "object-path": "^0.11.4",
    "@types/object-path": "^0.9.29",
    "ngx-bootstrap": "^3.0.1",
    "ngx-cookie-service": "^1.0.10",
    "ngx-dynamic-form-builder": "^0.4.3",
    "ngx-permissions": "^5.0.0",
    "ngx-repository": "^0.4.1",
    "node-fetch": "^2.1.2",
    "rxjs": "^6.0.0",
    "ts-loader": "4.2.0",
    "webpack-cli": "^2.1.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@rucken/cli": "^3.0.0-rc.4",
    "@types/body-parser": "^1.17.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "bootstrap-sass": "^3.3.7",
    "bootswatch-sass": "^3.2.0",
    "codelyzer": "~4.3.0",
    "conventional-changelog-cli": "^2.0.1",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cp-cli": "^1.1.2",
    "domino": "^2.0.2",
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "lite-server": "^2.4.0",
    "markdown-loader": "^3.0.0",
    "ng-packagr": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "rxjs",
      "ts-loader",
      "webpack-cli",
      "zone.js"
    ]
  }
}
