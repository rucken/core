{
  "name": "rucken",
  "version": "3.9.5",
  "engines": {
    "node": ">=11",
    "npm": ">=6.5.0"
  },
  "private": true,
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "description": "Core and Admin UI for Angular7+ web applications based on Rucken template",
  "bugs": {
    "url": "https://github.com/rucken/core/issues"
  },
  "homepage": "https://github.com/rucken/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/core.git"
  },
  "keywords": [
    "ngx",
    "angular",
    "angular7",
    "ngx-bootstrap",
    "admin ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap",
    "ssr"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-core": "ng build core --prod",
    "lib:build-web": "ng build web --prod",
    "lib:build": "run-s prepare:prod lib:build-core lib:build-web",
    "app:prebuild": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng build demo --prod -c=production",
    "app:prebuild:dev": "ng build demo --prod -c=dev",
    "app:build": "run-s prepare:mono app:prebuild",
    "app:build:dev": "run-s prepare:mono app:prebuild:dev",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve -c=production --port=8000 --host 0.0.0.0 --disable-host-check",
    "serve:dev": "ng serve -c=dev --port=8000 --host 0.0.0.0 --disable-host-check",
    "start": "run-s ssr:start",
    "start:prod": "run-s prepare:mono serve",
    "start:dev": "run-s prepare:mono serve:dev",
    "build": "run-s clean format:fix config:mono app:build ssr:build",
    "build:dev": "run-s clean format:fix config:mono app:build:dev ssr:build:dev",
    "test": "run-s build",
    "lint": "ng lint",
    "clean": "./node_modules/.bin/del-cli dist",
    "e2e": "ng e2e",
    "update": "ng update --all",
    "ssr:prebuild": "node --max_old_space_size=1024 ./node_modules/@angular/cli/bin/ng run demo:server:production",
    "ssr:prebuild:dev": "ng run demo:server:dev",
    "ssr:webpack": "webpack --progress --colors",
    "ssr:start": "node ./server.js",
    "ssr:build": "run-s ssr:prebuild ssr:webpack",
    "ssr:build:dev": "run-s ssr:prebuild:dev ssr:webpack",
    "postinstall": "node ./scripts/patch.js",
    "github:prebuild": "ng build demo --base-href /core/ --prod",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo-client/index.html ./dist/demo-client/404.html",
    "github:build": "run-s lib:build config:prod github:prebuild github:postbuild doc",
    "github:deploy": "gh-pages -d dist/demo-client",
    "doc:lib:core": "./node_modules/.bin/compodoc -p libs/rucken/core/tsconfig.lib.json -n \"rucken:core\" -d dist/demo-client/doc/core",
    "doc:lib:web": "./node_modules/.bin/compodoc -p libs/rucken/web/tsconfig.lib.json -n \"rucken:web\" -d dist/demo-client/doc/web",
    "doc:app:demo": "./node_modules/.bin/compodoc -p apps/demo/tsconfig.app.json -n \"rucken:demo\" -d dist/demo-client/doc/demo",
    "doc": "run-s doc:lib:core doc:lib:web doc:app:demo",
    "format:fix": "prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\""
  },
  "dependencies": {
    "@angular/animations": "^7.2.5",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/http": "^7.2.5",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/platform-server": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@ngx-meta/core": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.18",
    "@types/object-path": "^0.11.0",
    "bind-observable": "^1.0.2",
    "core-js": "^2.6.5",
    "cors": "~2.8.5",
    "dotenv": "^6.2.0",
    "jsonwebtoken": "^8.4.0",
    "ngx-bootstrap": "^3.2.0",
    "ngx-cookie-service": "^2.1.0",
    "ngx-dynamic-form-builder": "^0.9.0",
    "ngx-permissions": "^6.0.5",
    "ngx-repository": "^0.6.3",
    "node-fetch": "^2.3.0",
    "object-path": "^0.11.4",
    "raw-loader": "^0.5.1",
    "rxjs": "^6.4.0",
    "ts-loader": "5.3.3",
    "webpack-cli": "^3.2.1",
    "zone.js": "^0.8.29",
    "ngx-bind-io": "^0.6.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.1",
    "@angular-devkit/build-ng-packagr": "~0.13.1",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@compodoc/compodoc": "^1.1.8",
    "@rucken/cli": "^3.3.3",
    "@types/body-parser": "^1.17.0",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "bootstrap-sass": "^3.4.1",
    "bootswatch-sass": "^3.2.0",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "^2.0.12",
    "conventional-commits-detector": "^1.0.2",
    "conventional-recommended-bump": "^4.0.4",
    "cookie-parser": "^1.4.4",
    "cp-cli": "^2.0.0",
    "del-cli": "^1.1.0",
    "domino": "^2.1.2",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lite-server": "^2.4.0",
    "markdown-loader": "^5.0.0",
    "ng-packagr": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.2.4"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "rxjs",
      "ts-loader",
      "webpack-cli",
      "zone.js"
    ]
  }
}
